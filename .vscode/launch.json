{
  "configurations": [
    {
      "name": "Python: Current File",
      "type": "python",
      "request": "launch",
      "program": "scripts/${fileBasename}",
      "console": "integratedTerminal",
      "cwd": "${workspaceFolder}"
    },
    {
      "name": "crawl: VOZ STOCK",
      "type": "python",
      "request": "launch",
      "module": "scrapy",
      "args": ["crawl", "voz_stock", "-o", "data/comments-vscode.json"],
      "cwd": "${workspaceFolder}",
      "console": "integratedTerminal"
    }
  ]
}
